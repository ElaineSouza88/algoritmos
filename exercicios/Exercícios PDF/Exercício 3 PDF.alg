algoritmo "Exercicio3PDF"
var
   Nome : caractere
   Salario : real
inicio
      escreval ("Olá funcionário, precisamos de alguns dados, antes de iniciar.")
      escreva ("Digite seu nome: ")
      Leia (Nome)
      escreva ("Digite seu salário atual: ")
      leia (Salario)
      escreva ("Olá ", Nome, " ,atualmente você possui o salário de: ",Salario)
fimalgoritmo
