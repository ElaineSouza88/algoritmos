algoritmo "Exercicio5PDF"
var
   MSG: caractere
   N1, N2, M: REAL
inicio
    Escreval ("Vamos calcular a média de suas notas? ")
    Escreva ("Digite sua primeira nota: ")
    leia (N1)
    Escreva ("Digite sua segunda nota: ")
    leia (N2)
    M <- (N1 + N2) / 2
    ESCREVA ("A sua média é de: ", M)
fimalgoritmo
