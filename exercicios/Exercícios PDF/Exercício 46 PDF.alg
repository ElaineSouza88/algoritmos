algoritmo "Exercicio46PDF"
// Função : Programa que conte de 2 em 2 até 100, iniciando do 6 e realizando a soma entre os valores

var
   C, S: INTEIRO
inicio
      C <- 6
      S <- 0
      Enquanto (C <= 100) FACA
               EscrevaL (C)
               S <- S + C
               C <- C + 2
      FimEnquanto
      Escreva ("A soma de 6 a 500 é:",S)
fimalgoritmo
