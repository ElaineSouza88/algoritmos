algoritmo "Exercicio18PDF"

var
   A, I: INTEIRO
inicio
      Escreva ("Em que ano você nasceu? ")
      Leia (A)
      I <- 2024 - A
      Escreval ("Em 2024 você tem ",I," anos de idade.")
      Se (I >= 15) entao
         Escreva ("Você está apto a votar!")
      Senao
           Escreva ("A maior idade para voto é de 15 anos!")
      FimSe
fimalgoritmo
