algoritmo "Exercicio41PDF"
// Função :Programa que conte até o nº definido pelo usuário.

var
   C, V: INTEIRO
inicio
      C <- 0
      Escreva ("Digite um valor: ")
      Leia (V)
      Enquanto (C <= V) faca
               Escreval (C)
               C <- C + 1
      FimEnquanto
      Escreva ("Sua contagem foi até ",V,".")
fimalgoritmo
