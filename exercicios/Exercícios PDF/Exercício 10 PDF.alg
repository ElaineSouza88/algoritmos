algoritmo "Exercicio10PDF"

var
   L, A, AR, B: real
inicio
      Escreva ("Qual a largura da parede? ")
      Leia (L)
      Escreva ("Qual a altura da parece? ")
      Leia (A)
      AR <- L * A
      B <- AR / 2
      Escreval ("A área total é de ",AR," m2.")
      Escreva ("Para essa medida vamos precisar de ",B:5:2," baldes de tinta.")
fimalgoritmo
